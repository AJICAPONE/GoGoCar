section.drivers-nearby
    .container
        .drivers--wrap
            h3.main-section--title--v2.text-center Водители #[span рядом с Вами]
            p.section-desc Посмотрите, кто из водителей может забрать и высадить вас поблизости.
            .drivers-nearby--list.row
                .col-xl-4.col-lg-4.col-md-6.col-sm-6.gogocar-trip--item
                    .gogocar-trip--item__top
                        .gogocar-trip--item-town__route
                            .gogocar-trip--item__town
                                span Ташкент
                                span Андижан
                            .gogocar-trip--item__route
                                .gogocar-trip--item__route--start
                                .gogocar-trip--item__route--end
