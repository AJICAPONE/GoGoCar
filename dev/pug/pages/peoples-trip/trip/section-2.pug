                                    span 04:30
                                    .gogocar-drivers--time__icon.gdt-yellow
                                        +icon('location2')
                                .gogocar-drivers--time--end
                                    .gogocar-drivers--time__icon.gdt-green
                                        +icon('location2')
                                    span 10:10
                    .gogocar-trip--item__bottom
                        .gogocar-drivers--profile--wrap
