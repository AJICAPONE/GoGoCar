                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Дата:
                        input.gogocar-input--filter.popup-show-calendar.popup-show-calendar-click.calendar-zone-height(type="text" readonly="readonly")
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Особенности бронирования:
                        .all-trip--popup--to--book
                            input#all-trip-checkbox.all-trip--popup-checkbox(type="checkbox")
                            label(for="all-trip-checkbox") Автоматическое подтверждение
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Точка отправления:
                        input.gogocar-input--filter(type="text" placeholder="Откуда")
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Точка назначения:
                        input.gogocar-input--filter(type="text" placeholder="Куда")
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Расстояние до машины от вас (км):
                        input.gogocar-input--filter(type="text" placeholder="Любое")
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Цена (UZS):
                        .all-trip--popup__time
                            .all-trip--popup__time--before
                                span от
                                input.gogocar-input--filter(type="text")
                            span.all-trip--space –
                            .all-trip--popup__time--after
                                span до
                                input.gogocar-input--filter(type="text")
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Кол-во свободных мест:
                        select.gogocar-select
                            option(value="1") 1
                            option(value="2") 2
                            option(value="3") 3
                            option(value="4") 4
                            option(value="5") 5
                            option(value="6") 6
                            option(value="7") 7
                            option(value="8") 8
                .col-xl-4.col-lg-4.col-md-6.col-sm-12.atpc-filter
                    .all-trip--popup__col
                        label Рейтинг водителя:
                        select.gogocar-select
                            option(value="1") 1
                            option(value="2") 2
                            option(value="3") 3
                            option(value="4") 4
                            option(value="5") 5
