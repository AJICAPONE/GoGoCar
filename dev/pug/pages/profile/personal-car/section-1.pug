section.personal-settings.pt-0
    .container
        .personal-settings--wrap
            .col-xl-3.col-lg-3.personal-sidebar
                include ../../profile/sidebar-profile
            .col-xl-9.col-lg-9.personal-content
                .personal-content--main
                    .personal-content--header
                        h3.personal-content--header--title Мой автомобиль
                    .personal-content--body
                        ul.personal-car--list.row
                            li.col-xl-4.col-lg-5.col-md-12.col-sm-12.gogocar-added-car-li
                                .personal-car--item.gogocar-box
                                    .personal-car--item--left
                                        .personal-car--item--left__top 00 123 NN
                                        .personal-car--item--left__bottom Audi A4

                                    .personal-car--item--right
                                        .gogocar-gray-icons.personal-car--item--setting
                                            +icon('setting-car')
                                        .gogocar-gray-icons.personal-car--item--delete.gogocar-red-icons
                                            +icon('delete2')
                            li.col-xl-4.col-lg-5.col-md-12.col-sm-12.gogocar-added-car-li
                                .personal-car--item.gogocar-box
                                    .personal-car--item--left
                                        .personal-car--item--left__top 01 456 MM
                                        .personal-car--item--left__bottom ГАЗель

                                    .personal-car--item--right
                                        .gogocar-gray-icons.personal-car--item--setting
                                            +icon('setting-car')
                                        .gogocar-gray-icons.personal-car--item--delete.gogocar-red-icons
                                            +icon('delete2')
                            li.col-xl-4.col-lg-5.col-md-12.col-sm-12.gogocar-added-car-li
                                .personal-car--item--add.gogocar-box
                                    +icon('add')
                                    span.personal-car--item--add__text Добавить авто



