                                        li.personal-pay--header--childen__item Ташкент → Андижан #[span.pp-date 05.06.2020]
                                        li.personal-pay--header--childen__item Ташкент → Андижан #[span.pp-date 05.06.2020]
                                li.personal-pay--header--item.text-right
                                    .personal-pay--header--item__name Описание/Сумма
                                    ul.personal-pay--header--childen
                                        li.personal-pay--header--childen__item В одну сторону #[span.pp-price 90 000 UZS]
                                        li.personal-pay--header--childen__item В одну сторону #[span.pp-price 90 000 UZS]
                                        li.personal-pay--header--childen__item Туда и обратно #[span.pp-price 90 000 UZS]
