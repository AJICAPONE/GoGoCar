                                .personal-reviews--item--wrap
                                    .personal-reviews--item--top
                                        .chat-content--review__trip--info
                                            .chat-content--review__trip--info__img(style="background-image:url('/static/img/content/drivers-avatar/driver1.png');")
                                            .chat-content--review__trip--name__trip
                                                span.chat-content--review__trip--name Матвей Федотов
                                                span.chat-content--review__trip--trip 01.06.2020
                                        .personal-reviews--trip--rating
                                            .personal-reviews--trip Андижан → Ташкент
                                            .gogocar-trips-category--rating.green-rating
                                                +icon('green-star')
                                                span 4,3
                                    .personal-reviews--trip.personal-reviews--trip--moble Андижан → Ташкент
